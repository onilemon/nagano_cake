<h1>配送先編集</h1>
<%= form_with model: @address do |f| %>
  <div class="field">
  <%= f.label :postal_code, "郵便番号" %><br>
  <%= f.text_field :postal_code %>
  </div>

  <div class="field">
  <%= f.label :address, "住所" %><br>
  <%= f.text_field :address %>
  </div>

  <div class="field">
  <%= f.label :name, "宛名" %><br>
  <%= f.text_field :name %>
  </div>

  <div class="actions">
    <%= f.submit "変更内容を保存" %>
  </div>
<% end %>